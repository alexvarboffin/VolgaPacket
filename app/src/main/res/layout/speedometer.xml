<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/speedometr_matrp_edgar"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_gravity="bottom|center"
	android:orientation="vertical">

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="horizontal">

		<FrameLayout
			android:id="@+id/turn_left_pov"
			android:layout_width="@dimen/_18sdp"
			android:layout_height="@dimen/_18sdp"
			android:layout_gravity="bottom"
			android:layout_marginRight="@dimen/_3sdp"
			android:layout_marginBottom="@dimen/_2sdp"
			android:visibility="gone">

			<ImageView
				android:id="@+id/turn_left_bg_pov"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/fraction_progress" />

			<ImageView
				android:layout_width="@dimen/_15sdp"
				android:layout_height="@dimen/_15sdp"
				android:layout_gravity="center"
				app:srcCompat="@drawable/material_ic_keyboard_arrow_left_black_24dp" />
		</FrameLayout>

		<FrameLayout
			android:id="@+id/turn_left"
			android:layout_width="@dimen/_25sdp"
			android:layout_height="@dimen/_18sdp"
			android:layout_gravity="bottom"
			android:layout_marginBottom="@dimen/_2sdp">

			<ImageView
				android:id="@+id/turn_left_bg"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/casino_jackpot_x_bg_2" />

			<ImageView
				android:layout_width="@dimen/_15sdp"
				android:layout_height="@dimen/_15sdp"
				android:layout_gravity="center"
				app:srcCompat="@drawable/ic_speed_in_engine_active" />
		</FrameLayout>

		<FrameLayout
			android:id="@+id/fuel_layout"
			android:layout_width="@dimen/_25sdp"
			android:layout_height="@dimen/_18sdp"
			android:layout_gravity="bottom"
			android:layout_marginBottom="@dimen/_2sdp">

			<ImageView
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/e2y_btn_go" />

			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:orientation="vertical">

				<ImageView
					android:id="@+id/fuel_image"
					android:layout_width="@dimen/_9sdp"
					android:layout_height="@dimen/_9sdp"
					android:layout_gravity="center_horizontal"
					app:srcCompat="@drawable/ic_speed_fuel" />

				<TextView
					android:id="@+id/fuel_text"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"
					android:layout_marginTop="@dimen/_1sdp"
					android:fontFamily="@font/ttnorms_extrabold"
					android:text="100"
					android:textColor="#ffffffff"
					android:textSize="@dimen/_5sdp" />
			</LinearLayout>
		</FrameLayout>

		<FrameLayout
			android:id="@+id/speed_layout"
			android:layout_width="@dimen/_60sdp"
			android:layout_height="@dimen/_50sdp"
			android:layout_marginStart="@dimen/_5sdp"
			android:layout_marginEnd="@dimen/_5sdp">

			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_horizontal"
				android:layout_marginTop="@dimen/_1sdp"
				android:orientation="vertical">

				<TextView
					android:id="@+id/speed_text"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"
					android:fontFamily="@font/ttnorms_bold"
					android:includeFontPadding="false"
					android:text="98"
					android:textColor="#ffffffff"
					android:textSize="@dimen/_18sdp" />

				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"
					android:fontFamily="@font/ttnorms_medium"
					android:text="@string/speedometer_km"
					android:textColor="#4dffffff"
					android:textSize="@dimen/_5sdp" />

				<FrameLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"
					android:layout_marginTop="@dimen/_1sdp"
					android:background="@drawable/e2y_btn_go"
					android:paddingStart="@dimen/_3sdp"
					android:paddingTop="@dimen/_1sdp"
					android:paddingEnd="@dimen/_3sdp"
					android:paddingBottom="@dimen/_1sdp">

					<TextView
						android:id="@+id/mileage"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:fontFamily="@font/ttnorms_medium"
						android:text="000864"
						android:textColor="#ffffffff"
						android:textSize="@dimen/_6sdp" />
				</FrameLayout>

				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="@dimen/_3sdp"
					android:divider="@drawable/_5sdp_divider"
					android:orientation="horizontal"
					android:showDividers="middle">

					<ImageView
						android:id="@+id/in_light"
						android:layout_width="@dimen/_6sdp"
						android:layout_height="@dimen/_6sdp"
						app:srcCompat="@drawable/ic_speed_in_light" />

					<ImageView
						android:id="@+id/in_engine"
						android:layout_width="@dimen/_6sdp"
						android:layout_height="@dimen/_6sdp"
						app:srcCompat="@drawable/ic_speed_in_engine" />

					<ImageView
						android:id="@+id/in_key"
						android:layout_width="@dimen/_6sdp"
						android:layout_height="@dimen/_6sdp"
						app:srcCompat="@drawable/ic_speed_in_key" />
				</LinearLayout>

				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:divider="@drawable/_5sdp_divider"
					android:orientation="horizontal"
					android:showDividers="middle"
					android:visibility="gone">

					<ImageView
						android:id="@+id/in_left"
						android:layout_width="@dimen/_6sdp"
						android:layout_height="@dimen/_6sdp"
						app:srcCompat="@drawable/ic_speed_in_left" />

					<ImageView
						android:id="@+id/in_alarm"
						android:layout_width="@dimen/_6sdp"
						android:layout_height="@dimen/_6sdp"
						app:srcCompat="@drawable/ic_speed_in_alarm" />

					<ImageView
						android:id="@+id/in_right"
						android:layout_width="@dimen/_6sdp"
						android:layout_height="@dimen/_6sdp"
						app:srcCompat="@drawable/ic_speed_in_right" />
				</LinearLayout>

			</LinearLayout>

		</FrameLayout>

		<FrameLayout
			android:id="@+id/hp_layout"
			android:layout_width="@dimen/_25sdp"
			android:layout_height="@dimen/_18sdp"
			android:layout_gravity="bottom"
			android:layout_marginBottom="@dimen/_2sdp">

			<ImageView
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/e2y_prize_btn_bg" />

			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center"
				android:orientation="vertical">

				<ImageView
					android:layout_width="@dimen/_14sdp"
					android:layout_height="@dimen/_9sdp"
					android:layout_gravity="center_horizontal"
					app:srcCompat="@drawable/ic_speed_hp" />

				<TextView
					android:id="@+id/hp_text"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"
					android:layout_marginTop="@dimen/_1sdp"
					android:fontFamily="@font/ttnorms_extrabold"
					android:text="100"
					android:textColor="#ffffffff"
					android:textSize="@dimen/_5sdp" />
			</LinearLayout>
		</FrameLayout>

		<FrameLayout
			android:id="@+id/turn_right"
			android:layout_width="@dimen/_25sdp"
			android:layout_height="@dimen/_18sdp"
			android:layout_gravity="bottom"
			android:layout_marginBottom="@dimen/_2sdp">

			<ImageView
				android:id="@+id/turn_right_bg"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/casino_jackpot_x_bg_2" />

			<ImageView
				android:layout_width="@dimen/_15sdp"
				android:layout_height="@dimen/_15sdp"
				android:layout_gravity="center"
				app:srcCompat="@drawable/ic_speed_in_light_active" />

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal"></LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"></LinearLayout>
		</FrameLayout>

		<FrameLayout
			android:id="@+id/turn_right_pov"
			android:layout_width="@dimen/_18sdp"
			android:layout_height="@dimen/_18sdp"
			android:layout_gravity="bottom"
			android:layout_marginLeft="@dimen/_3sdp"
			android:layout_marginBottom="@dimen/_2sdp"
			android:visibility="gone">

			<ImageView
				android:id="@+id/turn_right_bg_pov"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/fraction_progress" />

			<ImageView
				android:layout_width="@dimen/_15sdp"
				android:layout_height="@dimen/_15sdp"
				android:layout_gravity="center"
				app:srcCompat="@drawable/material_ic_keyboard_arrow_right_black_24dp" />
		</FrameLayout>

	</LinearLayout>

	<LinearLayout
		android:id="@+id/menuspeedlag"
		android:layout_width="match_parent"
		android:layout_height="@dimen/_15sdp"
		android:gravity="center_horizontal"
		android:orientation="horizontal"
		android:visibility="visible">

		<FrameLayout
			android:id="@+id/turn_up"
			android:layout_width="@dimen/_15sdp"
			android:layout_height="@dimen/_15sdp"
			android:visibility="visible"
			tools:layout_gravity="bottom">

			<ImageView
				android:id="@+id/turn_up_bg"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/fraction_button_nofill" />

			<ImageView
				android:layout_width="@dimen/_15sdp"
				android:layout_height="@dimen/_15sdp"
				android:layout_gravity="center"
				app:srcCompat="@drawable/mtrl_ic_arrow_drop_up" />
		</FrameLayout>

		<FrameLayout
			android:id="@+id/turn_down"
			android:layout_width="@dimen/_15sdp"
			android:layout_height="@dimen/_15sdp"
			android:layout_marginBottom="@dimen/_31sdp"
			android:visibility="gone">

			<ImageView
				android:id="@+id/turn_down_bg"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/fraction_button_nofill" />

			<ImageView
				android:layout_width="@dimen/_15sdp"
				android:layout_height="@dimen/_15sdp"
				android:layout_gravity="center"
				app:srcCompat="@drawable/mtrl_ic_arrow_drop_down" />
		</FrameLayout>

		<FrameLayout
			android:id="@+id/turn_twin"
			android:layout_width="@dimen/_35sdp"
			android:layout_height="@dimen/_15sdp"
			android:layout_marginBottom="@dimen/_31sdp"
			android:visibility="gone">

			<ImageView
				android:id="@+id/turn_twin_bg"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/fraction_button_nofill" />

			<ImageView
				android:layout_width="@dimen/_11sdp"
				android:layout_height="@dimen/_12sdp"
				android:layout_gravity="center"
				app:srcCompat="@drawable/ic_radial_icon_35" />
		</FrameLayout>

		<FrameLayout
			android:id="@+id/turn_lock"
			android:layout_width="@dimen/_35sdp"
			android:layout_height="@dimen/_15sdp"
			android:layout_gravity="top"
			android:layout_marginBottom="@dimen/_31sdp"
			android:visibility="gone">

			<ImageView
				android:id="@+id/turn_lock_bg"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				app:srcCompat="@drawable/fraction_button_nofill" />

			<ImageView
				android:layout_width="@dimen/_12sdp"
				android:layout_height="@dimen/_12sdp"
				android:layout_gravity="center"
				app:srcCompat="@android:drawable/ic_lock_idle_lock" />
		</FrameLayout>

	</LinearLayout>

</LinearLayout>
